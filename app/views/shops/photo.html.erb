<%= form_tag("/shops/#{@shop.id}/photo", {multipart: true}) do %>
    <div class="form-group">
        <label class="control-label" for="InputPhoto">写真を追加</label>
        <div>
            <input type="file" name="photo" id="InputPhoto" class="form-control">
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="完了" class="btn btn-info col-md-2">
        <%= link_to '写真はいいや', shops_path, class: "btn btn-danger col-md-2" %>
    </div>
<% end %>